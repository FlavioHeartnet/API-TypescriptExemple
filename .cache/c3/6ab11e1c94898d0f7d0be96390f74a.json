source":"MessageList.tsx","name":null,"original":{"line":48,"column":1},"generated":{"line":74,"column":0}},{"source":"MessageList.tsx","name":null,"original":{"line":8,"column":0},"generated":{"line":74,"column":1}},{"source":"MessageList.tsx","name":null,"original":{"line":50,"column":15},"generated":{"line":76,"column":15}}],"sources":{"MessageList.tsx":"import { Map } from \"immutable\";\nimport React, { SyntheticEvent, useEffect, useState } from \"react\";\n\nimport \"./MessageList.scss\";\n\nimport { IMessage } from \"../socket\";\n\nconst MessageList = ({ socket }) => {\n  const [messages, setMessages] = useState(Map());\n\n  useEffect(() => {\n    const messageListener = (message: IMessage) => {\n      setMessages((prevMessages) => prevMessages.set(message.id, message));\n    };\n\n    const deleteMessageListener = (messageID: string) => {\n      setMessages((prevMessages) => prevMessages.delete(messageID));\n    };\n\n    socket.on(\"message\", messageListener);\n    socket.on(\"deleteMessage\", deleteMessageListener);\n    socket.emit(\"getMessages\");\n\n    return () => {\n      socket.off(\"message\", messageListener);\n      socket.off(\"deleteMessage\", deleteMessageListener);\n    };\n  }, [socket]);\n\n  return (\n    <div className=\"message-list\">\n      {messages\n        .toSet()\n        .sortBy((message: IMessage) => message.time)\n        .map((message: IMessage) => (\n          <div\n            key={message.id}\n            className=\"message-list--message-container\"\n            title={`Sent at ${new Date(message.time).toLocaleTimeString()}`}\n          >\n            <span className=\"message-list--message\">{message.value}</span>\n            <span className=\"message-list--user\">{message.user.name}</span>\n          </div>\n        )).toArray()\n      }\n    </div>\n  );\n};\n\nexport default MessageList;"},"lineCount":null}},"error":null,"hash":"4e86695dab46f395de3a5c732c50fa7d","cacheData":{"env":{}}}